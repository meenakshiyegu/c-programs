#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    char a[100],ans[10];
        int b[100];
    int du[20];
    int j=0,k=-1,l=0;
    scanf("%[^\n]s",a);
    int flag=0,i=0,ad=0,po=1;
    while(i<=strlen(a)){
        while(a[i]>=48 && a[i]<=57){
            ad=ad*po+(int)(a[i]-48);
            po=po*10;
            i+=1;
            flag=1;
        }
        if((a[i+1]>=48 && a[i+1]<=57) || ((i+1)>=strlen(a))){
            b[k]=ad;
            po=1;
            ad=0;
            k+=1;
        }
            if(flag==0)
                i+=1;
        flag=0;
        }
    l=0;
    flag=0;
    int four=0,three=0,two=0;
    for(i=0;i<k-1;i+=2){
        if(b[i]==3)three=1;
        if(b[i]==4)four=1;
        if(b[i]==2)two=1;
        for(j=0;j<k-1;j+=2){
            if(b[i]>b[j]){
                int temp=b[i];
                b[i]=b[j];
                b[j]=temp;
            }
        }
    }
    if(three==1 && four==1){
       printf("7");
        flag=1;
    }
    else if(three==1 && two==1){
        printf("5");
        flag=1;
    }
    else{
    for(int i=0;i<k-3;i+=2){
        if((b[i]!=9 && b[i+2]!=9)&& (b[i]-b[i+2]==1)){
            printf("%d",b[i]+b[i+2]);
            flag=1;
            break;
        }
    }
    if(flag==0)
        printf("undefined");
    }
}
/*
input
(10,9,8,7,4)
output
15
input
(10,9,8,3,4)
output
7
*/
