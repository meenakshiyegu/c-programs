#include<stdio.h>
#include<conio.h>
#include<string.h>
int main(){
	char a[100];
	int i,j,k=strlen(a)-1;
	clrscr();
	scanf("%[^\n]s",a);
	for(i=strlen(a)-1;i>=0;i--){
		if(a[i]==' '||i==-1){
			for(j=i+1;j<=k;j++){
				printf("%c",a[j]);
			}
			k=i;
			printf(" ");
		}
	}
	for(j=0;j<k;j++)
		printf("%c",a[j]);
	getch();
	return 0;
}
